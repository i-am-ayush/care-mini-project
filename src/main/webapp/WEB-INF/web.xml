<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <listener>
    <listener-class>listener.OnStart</listener-class>
  </listener>
  <servlet>
  <servlet-name>Login</servlet-name>
  <servlet-class>controller.servlet.Login</servlet-class>
</servlet>
<servlet>
<servlet-name>Logout</servlet-name>
<servlet-class>controller.servlet.Logout</servlet-class>
</servlet>

  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/controller.servlet.Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>Logout</servlet-name>
  <url-pattern>/controller.servlet.Logout</url-pattern>
  </servlet-mapping>
</web-app>
